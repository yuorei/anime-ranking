type User {
  userID: ID!
  name: String!
  password:String!
  relatedAnime:[AnimeRanking!]
}

type AnimeRanking {
  animeInformation:AnimeInformation!
  rank: Int!
  user: User!
}

type AnimeInformation {
  animeID: ID!
  title: String!
  imageURL: String!
  relatedAnime: [AnimeInformation!]
  registerUser: [User!]
}

input UserInformationInput {
  naem: String!
  password: String!
}

input NewAnimeRankingInput {
  title: String!
  imageURL: String!
  rank: Int!
  userID: ID!
}

type Query {
  GetUserInformation: [User!]!
  GetAnimeRanking: [AnimeRanking!]!
}

type Mutation {
  registerUser(input: UserInformationInput!): User!
  createAnimeRanking(input: NewAnimeRankingInput!): AnimeRanking!
}
